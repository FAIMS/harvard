/**************************** SW CORNER VALIDATION ****************************/
final String REF_TRENCH_SW_EASTING  = "Trench/Trench/Easting";
final String REF_TRENCH_SW_NORTHING = "Trench/Trench/Northing";

boolean validateSwCorner() {
  String val1 = getFieldValue(REF_TRENCH_SW_EASTING);
  String val2 = getFieldValue(REF_TRENCH_SW_EASTING);

  boolean isValid = !(isNull(val1) || isNull(val2));
  if (!isValid) {
    String head = "{sw_corner_validation_head}";
    String body = "{sw_corner_validation_body}";
    showWarning(head, body);
  }

  return isValid;
}

addOnEventCond(
    "Trench/Loci/Create_New_Locus",
    "click",
    "validateSwCorner()");
addOnEventCond(
    "Trench/Strata_Features/Create_New_Stratum_Feature",
    "click",
    "validateSwCorner()");
addOnEventCond(
    "Trench/FCNs/Create_New_FCN",
    "click",
    "validateSwCorner()");
