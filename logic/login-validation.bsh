/****************************** LOGIN VALIDATION ******************************/
// Overrides autogenerated function
void onClickUserUserListLogin () {
  String refUser = "User/User_List/Users";
  if (isNull(refUser)) {
    String head = "{Cannot_Log_in}";
    String body = "{You_must_select_a_user_to_log_in}";
    showWarning(head, body);

    return;
  }

  newTab("Control", true);
}
