<?xml version='1.0' encoding='utf-8'?>
<ValidationSchema>
  <ArchaeologicalElement name="Site">
    <property name="Trench ID">
      <validator type="blankchecker">
        <param type="field" value="measure"/>
      </validator>
    </property>
  </ArchaeologicalElement>
  <ArchaeologicalElement name="Trench">
    <property name="Excavation Method">
      <validator type="blankchecker">
        <param type="field" value="vocab"/>
      </validator>
    </property>
  </ArchaeologicalElement>
  <ArchaeologicalElement name="Context">
    <property name="Context Site Name">
      <validator type="blankchecker">
        <param type="field" value="measure"/>
      </validator>
    </property>
    <property name="Context Context ID">
      <validator type="blankchecker">
        <param type="field" value="measure"/>
      </validator>
    </property>
    <property name="Excavation Method">
      <validator type="blankchecker">
        <param type="field" value="vocab"/>
      </validator>
    </property>
    <property name="Highest Level">
      <validator type="blankchecker">
        <param type="field" value="measure"/>
      </validator>
    </property>
    <property name="Lowest Level">
      <validator type="blankchecker">
        <param type="field" value="measure"/>
      </validator>
    </property>
    <property name="Measure Length">
      <validator type="blankchecker">
        <param type="field" value="measure"/>
      </validator>
    </property>
    <property name="Measure Width">
      <validator type="blankchecker">
        <param type="field" value="measure"/>
